{
  "compilerOptions": {

    "target": "es2020",
    "lib": ["es2020", "DOM"],
    "module": "commonjs",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    "strict": true,
     "noImplicitAny": true,
     "strictNullChecks": true,
     "noUnusedLocals": true,
     "noUnusedParameters": true,

    "skipLibCheck": true
  }
}
